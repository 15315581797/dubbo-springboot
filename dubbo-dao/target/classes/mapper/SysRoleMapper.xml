<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.inspur.mybatis.mapper.SysRoleMapper">

    <select id="findUser" parameterType="long" resultType="com.inspur.domain.model.User">
        SELECT int_id,USERNAME,address FROM STA_USER WHERE int_id=#{0}
    </select>


</mapper>